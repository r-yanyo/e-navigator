<h2>新規面接作成</h2>

<%= form_for(@interview, url: user_interviews_path, html: { method: :post }) do |f| %>

  <div class="field">
    <%= f.label :date %><br />
    <%= f.datetime_select :date, :use_month_numbers => true,:start_year => Time.current.year%>
  </div>

   <div class="field">
    <%= f.hidden_field :user_id, :value => @user.id %><br />
  </div>

  <div class="actions">
    <%= f.submit "create" %>
  </div>
<% end %>
